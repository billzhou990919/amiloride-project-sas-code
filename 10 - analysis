proc freq data=cms.study_cohort_v2;
	table amiloride * pd;
run;
proc freq data=cms.study_cohort_v2;
	table eplerenone * pd;
run;
proc freq data=cms.study_cohort_v2;
	table spironolactone * pd;
run;
proc freq data=cms.study_cohort_v2;
	table triamterene * pd;
run;
proc freq data=cms.study_cohort_v2;
	table other_psdiuretics * pd;
run;


proc freq data=cms.study_cohort_prevalent_v2;
	table amiloride * pd;
run;
proc freq data=cms.study_cohort_prevalent_v2;
	table eplerenone * pd;
run;
proc freq data=cms.study_cohort_prevalent_v2;
	table spironolactone * pd;
run;
proc freq data=cms.study_cohort_prevalent_v2;
	table triamterene * pd;
run;
proc freq data=cms.study_cohort_prevalent_v2;
	table other_psdiuretics * pd;
run;

data cms.study_cohort_v2;
	set cms.study_cohort_v2;
	age = intck('year', birth_date, index_date);
run;
proc means data=cms.study_cohort_v2 mean;
    var age;
    where amiloride = 1;
run;
proc means data=cms.study_cohort_v2 mean;
    var age;
    where eplerenone = 1;
run;
proc means data=cms.study_cohort_v2 mean;
    var age;
    where spironolactone = 1;
run;
proc means data=cms.study_cohort_v2 mean;
    var age;
    where triamterene = 1;
run;
proc means data=cms.study_cohort_v2 mean;
    var age;
    where other_psdiuretics = 1;
run;

proc means data=cms.study_cohort_v2 sum;
    var survival_time;
    where amiloride = 1;
run;
proc means data=cms.study_cohort_v2 sum;
    var survival_time;
    where eplerenone = 1;
run;
proc means data=cms.study_cohort_v2 sum;
    var survival_time;
    where spironolactone = 1;
run;
proc means data=cms.study_cohort_v2 sum;
    var survival_time;
    where triamterene = 1;
run;
proc means data=cms.study_cohort_v2 sum;
    var survival_time;
    where other_psdiuretics = 1;
run;


proc phreg data=cms.study_cohort_v2;;
	class amiloride (ref='1')/ param=ref ref=first; 
	model survival_time*pd(0) = amiloride age / eventcode=1 risklimits;
run;
proc phreg data=cms.study_cohort_2017_cvd;;
	class psdiuretics_arb (ref='1')/ param=ref ref=first; 
	model survival_time*pd(0) = psdiuretics_arb age / eventcode=1 risklimits;
run;















data cms.study_cohort_prevalent_v2;
	set cms.study_cohort_prevalent_v2;
	age = intck('year', birth_date, index_date);
run;
proc means data=cms.study_cohort_prevalent_v2 mean;
    var age;
    where amiloride = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 mean;
    var age;
    where eplerenone = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 mean;
    var age;
    where spironolactone = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 mean;
    var age;
    where triamterene = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 mean;
    var age;
    where other_psdiuretics = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 sum;
    var survival_time;
    where amiloride = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 sum;
    var survival_time;
    where eplerenone = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 sum;
    var survival_time;
    where spironolactone = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 sum;
    var survival_time;
    where triamterene = 1;
run;
proc means data=cms.study_cohort_prevalent_v2 sum;
    var survival_time;
    where other_psdiuretics = 1;
run;


